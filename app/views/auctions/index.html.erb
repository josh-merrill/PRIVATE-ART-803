<h1>Upcoming Auctions</h1>
<div class="row my-5">
<ul>
  <% @auctions.each do |auction|%>
    <h3><%= auction.title %> </h3>
    <b><%= auction.address %> / <%= auction.date %></b>
    <p><%= auction.description %> / <%= auction.user.first_name %> <%= auction.user.last_name %></p>
  <% end %>
</ul>
</div>
