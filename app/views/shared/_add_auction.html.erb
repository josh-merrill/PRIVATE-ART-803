
<%= simple_form_for @auction do |f| %>
      <%= f.input :title %>
      <%= f.input :description %>
      <%= f.input :address %>
      <%= f.input :date %>
      <%= f.input :status %>
      <%= f.input :start_time %>
      <%= f.input :end_time %>
      <%= f.input :photo, as: :file %>
      <%= f.submit "Submit", class: "btn btn-primary w-100" %>
    <% end %>
