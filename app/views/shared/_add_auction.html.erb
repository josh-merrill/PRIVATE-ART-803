
<%= simple_form_for @auction do |f| %>
      <%= f.input :description %>
      <%= f.input :address %>
      <%= f.input :date %>
      <%= f.input :status %>
      <%= f.input :start_time %>
      <%= f.input :end_time %>
      <%= f.submit "Submit", class: "btn btn-dark rounded-pill" %>
    <% end %>
